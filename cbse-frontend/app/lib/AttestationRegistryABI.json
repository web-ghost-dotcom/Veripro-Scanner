pragma solidity ^0.8.0;

contract AttestationRegistry {
    event VerificationAttested(
        bytes32 indexed resultHash,
        address indexed prover,
        bool passed
    );

    function commitAttestation(
        bytes32 resultHash,
        bool passed,
        bytes32 contractHash,
        uint8 v,
        bytes32 r,
        bytes32 s
    ) external {}
}